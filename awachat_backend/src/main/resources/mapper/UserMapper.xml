<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kitra.awachat.next.mapper.UserMapper">

    <insert id="insertUser" parameterType="kitra.awachat.next.entity.UserEntity">
        INSERT INTO "user" (username, nickname, password)
        VALUES (#{username}, #{nickname}, #{password})
    </insert>

</mapper>